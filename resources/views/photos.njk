{% extends "master" %}

{% block content %}
<div class="app">
  <div class="app__header">
    <nav class="top-nav">
      <div class="container__nav">
        <button class="nav-btn" type="button" name="button">
          <i class="fa fa-plus-circle" aria-hidden="true"></i>
        </button>
      </div>
    </nav>
  {# </div> #}

    <div class="container__form">
      <form class="form" action="index.html" method="post">
        <div class="input-wrapper">
          <input class="form-input" type="text" name="name" value="" placeholder="Image URL">
          <input class="form-input" type="text" name="name" value="" placeholder="Image Caption">
        </div>
        <div class="btn-wrapper">
          <button class="btn cancel-btn"type="clear" name="button">Cancel</button>
          <button class="btn submit-btn"type="submit" name="button"><i class="fa fa-picture-o" aria-hidden="true"></i> Add Image</button>
        </div>
      </form>
    </div>
  </div>

  <div class="app__main">
    <div class="container">
      <ul class="photos-list">
        <li class="photos-list__item">
          <div class="image-wrapper">
            <img class="image" src="http://placekitten.com/300/300" alt="" />
          </div>
          <div class="caption-wrapper">
            <span class="caption">Caption</span>
          </div>
        </li>
      </ul>
    </div>
  </div>
</div>

{% endblock %}
